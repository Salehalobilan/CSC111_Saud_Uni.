import java.util.Scanner;

public class Sum {
	
	
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int a,b, sum;
		a = input.nextInt();
		System.out.println(a);
		b = input.nextInt();
		System.out.println(a);
		sum = input.nextInt();
		System.out.println(a + " + " + b + " = " + sum);

	}

}
